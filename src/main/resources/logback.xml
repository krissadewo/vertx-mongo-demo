<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <appender name="consoleAppender" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{yyyy-MM-dd HH:mm:ss} %green([%thread]) %highlight(%level) %logger{50}.%M\(%line\) - %msg%n
            </pattern>
        </encoder>
    </appender>

    <root level="TRACE">
        <appender-ref ref="consoleAppender"/>
    </root>

</configuration>
